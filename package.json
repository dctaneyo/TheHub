{
  "name": "the-hub",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "node -e \"require('fs').writeFileSync('build-id.txt', process.env.RAILWAY_GIT_COMMIT_SHA || String(Date.now()))\" && next build",
    "start": "node scripts/migrate.js && npx tsx src/lib/startup.ts && npx tsx server.ts",
    "lint": "eslint",
    "db:seed": "npx tsx src/lib/db/seed.ts",
    "db:push": "npx drizzle-kit push",
    "db:backup": "npx tsx scripts/backup-database.ts backup",
    "db:list-backups": "npx tsx scripts/backup-database.ts list",
    "db:restore": "npx tsx scripts/backup-database.ts restore",
    "db:add-indexes": "npx tsx scripts/add-indexes.ts",
    "push:keys": "npx tsx scripts/generate-vapid-keys.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "@livekit/components-react": "^2.9.20",
    "@livekit/components-styles": "^1.2.0",
    "@livekit/track-processors": "^0.7.0",
    "@sendgrid/mail": "^8.1.6",
    "@sentry/nextjs": "^10.40.0",
    "@shiguredo/rnnoise-wasm": "^2025.1.5",
    "@types/web-push": "^3.6.4",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "emoji-picker-react": "^4.18.0",
    "framer-motion": "^12.34.1",
    "jsonwebtoken": "^9.0.3",
    "livekit-client": "^2.17.2",
    "livekit-server-sdk": "^2.15.0",
    "lodash": "^4.17.23",
    "lucide-react": "^0.574.0",
    "next": "16.1.6",
    "node-cron": "^4.2.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.4.1",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie": "^0.6.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.23",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.3.1",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
